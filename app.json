{
  "expo": {
    "name": "Expogram",
    "description": "Dev photo sharing app",
    "slug": "Expogram",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/brand/icon.png",
    "splash": {
      "image": "./assets/brand/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": { "build": { "report": false } }
  }
}
